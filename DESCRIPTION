Package: cjar
Type: Package
Title: R Client for 'Customer Journey Analytics' ('CJA') API
Version: 0.1.3
Authors@R: c( 
    person(given = "Ben", family = "Woodard", role = c("aut", "ctb"), email = "benrwoodard@gmail.com"),
